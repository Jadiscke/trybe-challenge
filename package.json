{
  "name": "trybe-challenge",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon -L",
    "migration:generate": "yarn sequelize-cli migration:generate",
    "migrate": "yarn sequelize-cli db:migrate",
